<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonImIntent"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="115dp"
        android:layout_marginBottom="35dp"
        android:text="mayank vachhani image with implisite"
        app:layout_constraintBottom_toTopOf="@+id/edURL"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonExIn"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="386dp"
        android:text="call mayank vachhani"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edURL"
        tools:layout_editor_absoluteX="154dp" />

    <EditText
        android:id="@+id/edURL"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/buttonExIn"
        app:layout_constraintTop_toBottomOf="@+id/buttonImIntent" />
</androidx.constraintlayout.widget.ConstraintLayout>